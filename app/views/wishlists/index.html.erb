
<div class="property-item-wrapper bg-white">
  <div class="mx-auto max-w-2xl px-4 py-8 sm:px-6 sm:py-16 lg:max-w-7xl lg:px-8">
  <h2 class="text-3xl font-medium leading-12 text-gray-900 ">Wishlists</h2>
   <div class="mt-8 grid grid-cols-1 gap-x-6 gap-y-10 sm:grid-cols-2 lg:grid-cols-4 xl:gap-x-8">
      <% @properties.each do |property| %>
      <%= render 'properties/property_item', property: property, minimal_details: true %>
    <%end%>
    </div>
  </div>
</div>